{
  "name": "dispatch",
  "version": "0.2.0",
  "description": "AI agent pipeline orchestrator",
  "type": "module",
  "bin": {
    "dp": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsc && node -e \"const fs=require('fs');const f='dist/cli.js';const c=fs.readFileSync(f,'utf-8').replace(/^#!.*\\n/,'');fs.writeFileSync(f,'#!/usr/bin/env node\\n'+c)\"",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "link": "npm run build && npm link"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "gray-matter": "^4.0.3",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.7.0",
    "vitest": "^3.0.0"
  }
}
